<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cafe94.group14a2.LoginController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label text="Enter Your Email" translateX="-25.0" translateY="20.0" />
   <TextField fx:id="EmailTextField" promptText="Enter Email" />
    <Button onAction="#onLoginClick" text="Login" />
   <Button mnemonicParsing="false" onAction="#onSignUpClick" text="Create Account" />
   <Label fx:id="LoginErrorLabel" textFill="#ea0000" />
</VBox>
